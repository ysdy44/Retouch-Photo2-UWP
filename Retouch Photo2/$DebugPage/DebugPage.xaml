    <Grid Background="{ThemeResource AppBackgroundDark}">
        <Grid.RowDefinitions>
            <RowDefinition Height="50"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>


        <!--Extensions-->
        <elements:ApplicationTitleBarExtension x:Name="ApplicationView" TitleBarColor="{ThemeResource AppBackgroundColorMediumLight}"/>


        <!--Developer-->
        <developerTools:AlignmentGrid Grid.RowSpan="2" Opacity="0.15" HorizontalStep="20" VerticalStep="20" LineBrush="Gray" Visibility="{ThemeResource AppGridVisibility}" />


        <!--Animated-->
        <canvas:CanvasAnimatedControl x:Name="CanvasAnimatedControl" Grid.RowSpan="2"/>


        <!--Body-->
        <ScrollViewer x:Name="ScrollViewer" Grid.RowSpan="2" Padding="20,0">
            <Border>
                <GridView                      
                    MaxWidth="800" 
                    IsItemClickEnabled="False" 
                    SelectionMode="None" 
                    ReorderMode="Enabled" 
                    CanReorderItems="True"
                    CanDragItems="True" 
                    AllowDrop="True"
                    ItemsSource="{x:Bind CollectionViewSource.View}" 
                    animations:ItemsReorderAnimation.Duration="0:0:0.3">

                    <GridView.ItemContainerTransitions>
                        <TransitionCollection>
                            <EntranceThemeTransition IsStaggeringEnabled="True"/>
                        </TransitionCollection>
                    </GridView.ItemContainerTransitions>

                    <GridView.ItemContainerStyle>
                        <Style TargetType="GridViewItem">
                            <Setter Property="Margin" Value="1"/>
                            <Setter Property="Width" Value="100"/>
                            <Setter Property="Height" Value="Auto"/>
                            <Setter Property="Padding" Value="12,2"/>
                            <Setter Property="HorizontalContentAlignment" Value="Stretch"/>
                        </Style>
                    </GridView.ItemContainerStyle>

                    <GridView.GroupStyle>
                        <GroupStyle>
                            <GroupStyle.HeaderTemplate>
                                <DataTemplate x:DataType="local:UIElementGrouping">
                                    <TextBlock Text="{x:Bind Title}" Style="{StaticResource AppTabTextBlockStyle}"/>
                                </DataTemplate>
                            </GroupStyle.HeaderTemplate>
                        </GroupStyle>
                    </GridView.GroupStyle>

                    <GridView.Header>
                        <ContentControl HorizontalAlignment="Center" ContentTemplate="{StaticResource AppListViewBaseHeader}"/>
                    </GridView.Header>

                    <GridView.Footer>
                        <ContentControl HorizontalAlignment="Center" ContentTemplate="{StaticResource AppListViewBaseFooter}"/>
                    </GridView.Footer>

                </GridView>
            </Border>
        </ScrollViewer>


        <!--Head-->
        <elements:ExpanedHead x:Name="Head">
            <elements:ExpanedHead.LeftButtonContent>
                <FontIcon Glyph="&#xE830;" FontSize="18" FontWeight="Bold" FontFamily="{ThemeResource SymbolThemeFontFamily}" 
                          MirroredWhenRightToLeft="True"/>
            </elements:ExpanedHead.LeftButtonContent>
            <elements:ExpanedHead.RightButtonContent>
                <FontIcon Glyph="&#xED59;" FontSize="18" FontWeight="Bold" FontFamily="{ThemeResource SymbolThemeFontFamily}" 
                          MirroredWhenRightToLeft="True"/>
            </elements:ExpanedHead.RightButtonContent>
        </elements:ExpanedHead>

    </Grid>
</Page>